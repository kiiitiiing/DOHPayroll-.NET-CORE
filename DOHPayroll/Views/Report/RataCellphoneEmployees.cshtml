@model List<EmployeeCellphoneRata>

@if (Model.Count() > 0)
{
    <table class="table table-responsive">
        <tbody>
            @foreach(var employee in Model)
            {
                <tr>
                    <td class="hidden">
                        @employee.ID
                    </td>
                    <td>
                        @employee.Fullname
                    </td>
                    <td>
                        <span class="btn btn-sm btn-danger fa-pull-right" onclick="DeleteEmployee($(this).parent().parent().find('td:eq(0)').text().trim())">
                            <i class="fas fa-times"></i>
                        </span>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <div class="alert alert-warning">
        <span class="text-lg">
            <i class="fa fa-exclamation-triangle"></i>
            &nbsp;No data found!
        </span>
    </div>
}